{{#each playlists}}
  <section class="ui stacked segment">
    <h2 class="ui header">
      {{title}}
    </h2>
    <a href="/playlist/{{id}}" class="ui icon button">
      <i class="icon visit"></i>
    </a>
    <a href="/dashboard/deletesite/{{id}}" class="ui icon button">
      <i class="icon trash"></i>
    </a>
  </section>
{{/each}}

